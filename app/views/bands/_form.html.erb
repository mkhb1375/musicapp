
   <%= form_for band, html: { multipart: true } do |f| %>
      
      
      <%= f.label :name %><br>
      <%= f.text_field :name %><br>

      
        <%= f.label :cover %><br>
      <%= f.file_field :cover, accept: accept='image/png,image/jpeg' %><br>

      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>

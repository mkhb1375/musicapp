



   <%= form_for track, html: { multipart: true } do |f| %>
      
      
      <%= f.label :title %><br>
      <%= f.text_field :title %><br>

      <%= f.label :ord %><br>
      <%= f.text_field :ord %><br>

      <%= f.label :lyrics %><br>
      <%= f.text_area :lyrics , class:"lyrics"  %><br>
      <input class="band_form" type="hidden" name="[track]track_type"  value= "1" >
    
        <%= f.label :music %><br>
      <%= f.file_field :music, accept: accept="audio/*" %><br>

      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>
